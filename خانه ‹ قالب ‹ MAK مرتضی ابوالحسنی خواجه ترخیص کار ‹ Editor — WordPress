.wp-block-jetpack-layout-grid-column { max-width: 100%; }

.wpcom-overlay-grid { position: relative; inset: 0px; color: rgba(0, 0, 0, 0.03); }

.is-selected .wpcom-overlay-grid { position: absolute; }

